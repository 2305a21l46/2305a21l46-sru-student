            "enter the primary voltage in volts: 220\n",
            "enter the secondary voltage in volts: 440\n",
            "enter the open ckt test voltage in volts: 220\n",
            "enter the open ckt test current in amperes: 2\n",
            "enter the open ckt test power in watts: 250\n",
            "enter the short ckt test voltage in volts: 75\n",
            "enter the short ckt test current in amperes: 10\n",
            "enter the short ckt test power in watts: 500\n",
            "enter the power factor of the transformer: 1\n",
            "enter the loading of a transformer: 1\n",
            "primary voltage= 220.0 volts\n",
            "secondary voltage= 440.0 volts\n",
            "Ro= 193.6 ohms\n",
            "Xo= 133.67309875319802 ohms\n",
            "Ro1= 1.25 ohms\n",
            "Xo1= 1.3975424859373686 ohms\n",
            "core losses of transformer= 250.0 watts\n",
            "loading of the transformer= 1.0\n",
            "copper losses of transformer at loading 1.0 = 500.0 watts\n",
            "efficiency of transformer at loading= 86.95652173913044 %\n",
            "maximum efficiency loading= 0.7071067811865476\n",
            "no load phase angle of the transformer= 55.37646452082434 degress\n"
          ]
